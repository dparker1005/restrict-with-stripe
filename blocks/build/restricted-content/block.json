{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "rwstripe/restricted-content",
	"version": "1.0.0",
	"title": "Restricted Content",
	"category": "rwstripe",
	"icon": "privacy",
	"description": "Restrict individual blocks with Stripe products.",
	"attributes": {
		"rwstripe_restricted_products": {
			"type": "array",
			"default": []
		},
		"rwstripe_show_checkout_form": {
			"type": "boolean",
			"default": true
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "restrict-with-stripe",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
